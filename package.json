{
  "name": "temanashi-client",
  "homepage": "/",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/css": "^11.9.0",
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@firebase/firestore-types": "^2.5.0",
    "@hookform/resolvers": "^2.8.10",
    "@mui/base": "^5.0.0-alpha.85",
    "@mui/icons-material": "^5.6.2",
    "@mui/material": "^5.7.0",
    "@stripe/stripe-js": "^1.15.0",
    "@testing-library/cypress": "^8.0.2",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "@types/jest": "^27.0.3",
    "@types/node": "^16.11.12",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "@types/react-lazy-load-image-component": "^1.5.2",
    "@types/react-router-dom": "^5.3.2",
    "@types/react-scroll": "^1.8.3",
    "axios": "^0.25.0",
    "blueimp-load-image": "^5.16.0",
    "csstype": "^3.0.10",
    "date-fns": "^2.27.0",
    "dotenv-cli": "^6.0.0",
    "env-cmd": "^10.1.0",
    "firebase": "8.5",
    "hamburger-react": "^2.4.1",
    "node-gyp": "^9.0.0",
    "react": "^17.0.2",
    "react-cookie": "^4.1.1",
    "react-dom": "^17.0.2",
    "react-error-boundary": "^3.1.4",
    "react-helmet-async": "^1.3.0",
    "react-hook-form": "^7.31.3",
    "react-lazy-load-image-component": "^1.5.1",
    "react-loader-spinner": "^4.0.0",
    "react-responsive-carousel": "^3.2.22",
    "react-router-dom": "^5.3.0",
    "react-scripts": "^4.0.3",
    "react-scroll": "^1.8.7",
    "react-select": "^5.2.1",
    "react-tinder-card": "^1.4.5",
    "sass": "^1.45.0",
    "stripe": "^8.154.0",
    "typescript": "^4.5.3",
    "uuid": "^8.3.2",
    "web-vitals": "^1.1.2",
    "yup": "^0.32.11"
  },
  "scripts": {
    "start": "dotenv -e .env.development react-scripts start",
    "start:stg": "dotenv -e .env.staging react-scripts start",
    "build": "npm run clean && dotenv -e .env.development react-scripts build",
    "build:stg": "npm run clean && dotenv -e .env.staging react-scripts build",
    "deploy": "npm run build && firebase deploy --project=temanashi-phase2",
    "deploy:stg": "npm run build:stg && firebase deploy --project=temanashi-phase2-development",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "emulators": "cd functions && npm run emulators",
    "clean": "rimraf build",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "cy:run:chrome": "cypress run --browser chrome",
    "cy:run:firefox": "cypress run --browser firefox",
    "lint:css": "stylelint --ignore-path .gitignore \"src/**/*.+(css|ts|tsx)\"",
    "lint:css:fix": "npm run lint:css -- --fix",
    "lint:js": "./node_modules/.bin/eslint --ext .tsx,.ts src/",
    "lint:js:fix": "npm run lint:js -- --fix",
    "format": "prettier --write \"**/*.+(js|json|yml|ts|tsx)\"",
    "prepare": "husky install",
    "lint-staged": "lint-staged"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "npm run lint:js:fix",
      "npm run lint:css:fix",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@stylelint/postcss-css-in-js": "^0.38.0",
    "@types/blueimp-load-image": "^5.14.4",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "cross-env": "^7.0.3",
    "cypress": "^9.4.1",
    "cypress-file-upload": "^5.0.8",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "firebase-tools": "^10.2.0",
    "husky": "^8.0.0",
    "lint-staged": "^13.0.3",
    "postcss": "^8.4.14",
    "postcss-syntax": "^0.36.2",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "stylelint": "^14.9.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recommended": "^8.0.0",
    "stylelint-config-standard": "^26.0.0",
    "stylelint-order": "^5.0.0"
  }
}
